<div class="container">
<h1>Instagram clone<h1>
<hr>
<h2>Log in</h2>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <br>
    <%= f.submit "Log in" %>
<% end %>
</div>